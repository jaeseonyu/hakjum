<h1>Listing scores</h1>

<table>


<% @scores.each do |score| %>
  <tr>
    <td><%= score.current_average_score %></td>
    <td><%= score.need_average_score %></td>
    <td><%= score.safe_average_score %></td>
    <td><%=l score.created_at %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Score', new_score_path %>
